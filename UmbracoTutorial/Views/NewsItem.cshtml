@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.NewsItem>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "SubMaster.cshtml";
}

<div class="container">
    <h1>@Umbraco.Field("pageName")</h1>
    <h3>@Umbraco.Field("summary")</h3>
    @CurrentPage.GetGridHtml("bodyText")
    @section sidebar
    {
        @Html.Partial("sideNavigation")
    }
</div>

